    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-babili": "^0.1.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.0.0",
    "eslint": "^4.1.0",
    "eslint-config-prev": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
